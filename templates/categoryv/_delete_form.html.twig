<div class="container">
<form method="post" action="{{ path('categoryv_delete', {'id': categoryv.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categoryv.id) }}">
    <button class="btn">Delete</button>
</form>
</div>
