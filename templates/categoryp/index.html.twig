{% extends 'base.html.twig' %}

{% block title %}Categoryp index{% endblock %}

{% block body %}
    <div class="container">
    <h1>Categoryp index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categoryp in categoryps %}
            <tr>
                <td>{{ categoryp.id }}</td>
                <td>{{ categoryp.name }}</td>
                <td>
                    <a href="{{ path('categoryp_show', {'id': categoryp.id}) }}">show</a>
                    <a href="{{ path('categoryp_edit', {'id': categoryp.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('categoryp_new') }}">Create new</a>
    </div>
{% endblock %}
